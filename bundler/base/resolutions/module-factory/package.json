{
	"name": "@evmts/module-factory",
	"version": "0.11.2",
	"private": false,
	"license": "MIT",
	"type": "module",
	"exports": {
		"./package.json": "./package.json",
		".": {
			"import": "./baseBundler.js",
			"require": "./dist/baseBundler.cjs",
			"types": "./types/baseBundler.d.ts",
			"default": "./dist/baseBundler.cjs"
		}
	},
	"main": "dist/baseBundler.cjs",
	"module": "baseBundler.js",
	"types": "types/baseBundler.d.ts",
	"scripts": {
		"build": "nx run-many --targets=build:dist,build:types --projects=@evmts/base ",
		"build:dist": "bun run tsup",
		"build:types": "bun run tsc --emitDeclarationOnly --declaration --declarationMap",
		"clean": "rm -rf node_modules && rm -rf artifacts && rm -rf dist && rm -rf cache",
		"test": "vitest --coverage",
		"test:coverage": "vitest run --coverage",
		"test:run": "vitest run",
		"test:ui": "vitest --ui"
	},
	"dependencies": {
		"effect": "^2.0.0-next.54"
	},
	"devDependencies": {
		"@evmts/resolutions-types": "workspace:^",
		"@vitest/coverage-v8": "^0.34.6",
		"@vitest/ui": "^0.34.6",
		"tsup": "^7.2.0",
		"typescript": "^5.2.2",
		"vitest": "^0.34.6"
	},
	"peerDependencies": {
		"solc": ">0.8.10"
	},
	"publishConfig": {
		"access": "public"
	}
}
